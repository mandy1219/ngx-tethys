{
  "name": "ngx-tethys",
  "version": "7.6.25",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:ngm",
    "build:ng-pgr": "rimraf ./built/* ./codegen/* && ng build ngx-tethys && cpx \"./src/**/**/*.scss\" built",
    "build:ng": "ng build ngx-tethys --prod",
    "build:ngc": "rimraf ./built/* ./codegen/* && ngc -p ./src/tsconfig.lib.json",
    "build:ngm": "ngm build -p src --clean",
    "build:theme": "gulp build-theme",
    "build:theme:watch": "npm run build:theme && gulp build-theme:watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg src/package.json",
    "release": "node ./scripts/release.js",
    "release-next": "node ./scripts/release-next.js",
    "pub-only": "cd ./built && npm publish && cd ..",
    "pub": "npm run build && npm run pub-only",
    "ng": "ng",
    "start": "npm run build:theme && concurrently \"ng serve --port 8888\" \"gulp build-theme:watch\"",
    "build:demo": "npm run build:docs",
    "build:docs": "ng build demo --prod --extract-css --baseHref=/ngx-tethys/",
    "test": "ng test ngx-tethys",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve-demo": "http-server docs -p 8888 -g &"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/worktile/ngx-tethys.git"
  },
  "description": "Angular UI Components",
  "keywords": [
    "Angular",
    "Angular 2+"
  ],
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngx-translate/core": "^11.0.1",
    "angular-sortablejs": "^2.7.0",
    "bootstrap": "^4.4.1",
    "china-division": "^2.2.0",
    "core-js": "^3.6.4",
    "highlight.js": "^9.17.1",
    "natives": "^1.1.6",
    "ng-packagr": "^5.7.1",
    "ngx-bootstrap": "^5.3.2",
    "ngx-highlightjs": "^4.0.2",
    "node-sass": "^4.13.0",
    "rxjs": "^6.5.4",
    "sortablejs": "^1.10.2",
    "tsickle": "^0.38.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular-devkit/build-ng-packagr": "~0.803.22",
    "@angular/cli": "~8.3.22",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.1.6",
    "@types/webpack": "^4.41.2",
    "chalk": "^3.0.0",
    "codelyzer": "~5.2.1",
    "commander": "^4.1.0",
    "concurrently": "^5.0.2",
    "conventional-changelog-cli": "^2.0.31",
    "cpx": "^1.5.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "http-server": "^0.12.1",
    "husky": "^4.0.7",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ngm-cli": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.2",
    "readline-sync": "^1.4.10",
    "replace": "^1.1.5",
    "semver": "^7.1.1",
    "standard-version": "^7.0.1",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.1",
    "typescript": "3.5.2",
    "webpack": "^4.41.5"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run build",
      "postchangelog": "node ./scripts/remove-changelog-header.js"
    },
    "skip": {
      "commit": true,
      "tag": true
    }
  }
}
